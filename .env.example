# API Configuration
API_BASE_URL=https://api.example.com
API_TIMEOUT=30000

# Environment
# Options: development, staging, production
APP_ENV=development
NODE_ENV=development

# App Configuration
APP_SCHEME=template-mobile-app

# Visma Connect OAuth Configuration
# Get your client ID from https://developer.visma.com
VISMA_CONNECT_CLIENT_ID=your-client-id-here

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_CRASH_REPORTING=false

# Third-party Services (add your own)
# Sentry error tracking
# SENTRY_DSN=https://your-sentry-dsn@sentry.io/project

# Firebase Analytics (optional)
# FIREBASE_API_KEY=
# FIREBASE_APP_ID=

# EAS Build Configuration
# Get your project ID from expo.dev
# EAS_PROJECT_ID=your-project-id

# Expo Owner (your Expo username)
# EXPO_OWNER=your-expo-username
